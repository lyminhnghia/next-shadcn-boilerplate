{
  "extends": [
    "next/core-web-vitals",
    "plugin:storybook/recommended"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "import/no-unresolved": "off",
    "import/named": "off",
    "no-console": "warn",
    "react-hooks/exhaustive-deps": "warn"
  }
}